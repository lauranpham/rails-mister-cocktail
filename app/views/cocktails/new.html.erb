<div class="container">
  <h1>New Cocktail</h1>

  <div class="container">
    <%= simple_form_for(@cocktail) do |f| %>
      <%= f.input :name %>
      <%= f.input :photo, as: :file%>
      <%= f.submit class: "btn btn-primary" %>
    <% end %>

    <br>

    <%= link_to "Back", cocktails_path %>
  </div>

</div>
